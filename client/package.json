    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{js,jsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
